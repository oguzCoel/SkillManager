  			<version>2.18.1</version>
  		</plugin>
  	</plugins>
  </reporting>
  
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
  
  
 <build>
 <pluginManagement>
  <plugins>
    <plugin>
